<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    
    <!-- Light (300) -->
    <font
        android:fontStyle="normal"
        android:fontWeight="300"
        android:font="@font/outfit_light"
        app:fontStyle="normal"
        app:fontWeight="300"
        app:font="@font/outfit_light" />
    
    <!-- Regular (400) -->
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/outfit_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/outfit_regular" />
    
    <!-- Medium (500) -->
    <font
        android:fontStyle="normal"
        android:fontWeight="500"
        android:font="@font/outfit_medium"
        app:fontStyle="normal"
        app:fontWeight="500"
        app:font="@font/outfit_medium" />
    
    <!-- SemiBold (600) -->
    <font
        android:fontStyle="normal"
        android:fontWeight="600"
        android:font="@font/outfit_semibold"
        app:fontStyle="normal"
        app:fontWeight="600"
        app:font="@font/outfit_semibold" />
    
    <!-- Bold (700) -->
    <font
        android:fontStyle="normal"
        android:fontWeight="700"
        android:font="@font/outfit_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/outfit_bold" />
    
</font-family>